{
  "manifest_version": 3,
  "name": "__MSG_application_name__",
  "version": "0.1.0",
  "description": "__MSG_application_description__",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
